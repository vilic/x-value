{
  "root": true,
  "overrides": [
    {
      "files": ["**/*.{ts,tsx}"],
      "extends": ["plugin:@mufan/default"],
      "parserOptions": {
        "project": "tsconfig.json"
      },
      "rules": {
        "no-unused-vars": [
          "error",
          {
            "varsIgnorePattern": "^_([^_]|$)",
            "argsIgnorePattern": "^_([^_]|$)"
          }
        ]
      }
    }
  ]
}
